Create Database Company;
--Create DB Company and Table for customers.
CREATE TABLE Customers (
CustomerID varchar (255) not null AUTO_INCREMENT primary key,
LastName varchar (255),
FirstName varchar (255),
Address varchar (255),
Phone numeric (20)
);
--Insert customer into the table
INSERT INTO Customers (
LastName, FirstName, Address, Phone
)
